#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
int menu;
printf("Choose menu: \n");
printf("1. Coke \n");
printf("2. Orange \n");
printf("3. Mint \n");

scanf("%d", &menu);

if (menu ==1) {
printf("Coke is 2$");
}
else if (menu == 2) {
printf("Orange is 3$");
}
else if (menu == 3) {
printf("Mint is not exist now.");
}

return 0;
}
